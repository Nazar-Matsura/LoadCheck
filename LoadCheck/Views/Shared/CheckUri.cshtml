@model LoadCheck.Models.CheckUrlResults

<img src="data:image;base64,@System.Convert.ToBase64String(Model.ChartImage)"/>
<div class="row">
    <table class="table">
        <tr>
            <th>
                "Url"
            </th>
            <th>
                "Minimum Time"
            </th>
            <th>
                "Maximum Time"
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model.Data)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Url)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MinResponseTime)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MaxResponseTime)
                </td>
            </tr>
        }

    </table>
</div>


